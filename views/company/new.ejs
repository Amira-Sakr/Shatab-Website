

<h1>Creat a New Company to <%=category.name%></h1>


<form action="/category/<%=category._id%>/company" method="POST">
    <input type="text" name="company[name]" placeholder="name">
    <input type="text" name="company[image]" placeholder="img url">
    <input type="text" name="company[descr]" placeholder="description">
    
    <button>Submit!</button>


</form>
